# Environment Variables Template
# Copy this file to .env and fill in your actual values

# Database
POSTGRES_PASSWORD=your_postgres_password
DATABASE_URL=postgresql://postgres:your_password@localhost:5432/fairydust

# Redis
REDIS_URL=redis://localhost:6379/0

# JWT
JWT_SECRET_KEY=your_jwt_secret_key_change_this

# Email (Resend)
SMTP_PASSWORD=re_your_actual_resend_api_key

# SMS (Twilio)
TWILIO_ACCOUNT_SID=your_twilio_account_sid
TWILIO_AUTH_TOKEN=your_twilio_auth_token
TWILIO_PHONE_NUMBER=your_twilio_phone_number

# OAuth (if using)
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret

# Google Maps/Places API
GOOGLE_PLACES_API_KEY=your_google_places_api_key

# TripAdvisor API
TRIPADVISOR_API_KEY=your_tripadvisor_api_key

# AI API Keys
ANTHROPIC_API_KEY=your_anthropic_api_key
OPENAI_API_KEY=your_openai_api_key

# Free Astrology API
FREE_ASTROLOGY_API_KEY=your_free_astrology_api_key

# Environment
ENVIRONMENT=development
ALLOWED_ORIGINS=*,null,file://,http://localhost:3000,http://127.0.0.1:3000