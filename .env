# .env.example - Copy this to .env and fill in your values

# Database
DATABASE_URL=postgresql://postgres:password@localhost:5432/fairydust

# Redis
REDIS_URL=redis://localhost:6379/0

# JWT
JWT_SECRET_KEY=your-secret-key-here-change-in-production

# OAuth Providers (optional for now)
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=
GOOGLE_REDIRECT_URI=http://localhost:8001/auth/oauth/google/callback

# Email (optional for now)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USERNAME=
SMTP_PASSWORD=
FROM_EMAIL=noreply@fairydust.fun

# SMS (optional for now)
TWILIO_ACCOUNT_SID=
TWILIO_AUTH_TOKEN=
TWILIO_FROM_NUMBER=

# CORS
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:5173,https://fairydust.fun

# Environment
ENVIRONMENT=development
PORT=8001

# Service Auth
SERVICE_AUTH_TOKEN=dev-service-token